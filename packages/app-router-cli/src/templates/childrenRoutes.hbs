{{#each routes}}
{
  {{#if this.id}}
  id: "{{this.id}}",{{/if}}
  {{~#if this.path}}
  path: "{{this.path}}",{{/if}}
  {{~#if this.index}}
  index: {{this.index}},{{/if}}
  element: (
    {{~#if (isPageRoute this.type)}}{{> pageRouteElement this}}{{/if~}}
    {{~#if (isLayoutRoute this.type)}}{{> layoutRouteElement this}}{{/if~}}
    {{~#if (isInterceptedRoute this.type)}}{{> interceptedRouteElement this}}{{/if~}}
    {{~#if (isInterceptingRoute this.type)}}{{> interceptingRouteElement this}}{{/if~}}
  )
  {{~#unless (isNilorEmpty this.children)}},
  children: [{{> childrenRoutes routes=this.children}}]
  {{~/unless~}}
},
{{/each}}