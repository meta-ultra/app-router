{
  "name": "@meta-ultra/app-router-auth",
  "private": false,
  "version": "0.1.0",
  "description": "Authentication perfectly works with @meta-ultra/app-router",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "pnpm run \"/^dev:.*/\"",
    "dev:src": "rollup --watch -c ./rollup.config.js",
    "dev:types": "tsc --watch -P ./tsconfig.types.json",
    "build": "pnpm clean && pnpm run \"/^build:.*/\"",
    "build:src": "rollup -c ./rollup.config.js",
    "build:types": "tsc -P ./tsconfig.types.json"
  },
  "module": "./dist/bundle.esm.js",
  "main": "./dist/bundle.cjs.js",
  "typings": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist"
  ],
  "keywords": [
    "next.js",
    "react",
    "app router",
    "auth",
    "authentication"
  ],
  "author": "fsjohnhuang",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-react-constant-elements": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime-corejs3": "^7.22.5",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.5",
    "@types/react-is": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel": "^6.23.0",
    "babel-plugin-import": "^1.13.8",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^5.1.3",
    "typescript-plugin-css-modules": "^5.0.1"
  },
  "dependencies": {
    "@meta-ultra/app-router": "workspace:^",
    "react-is": "^18.2.0"
  }
}
